   "source": [
    "buildHeap(A[], n) {             # A[1~n]을 힙으로 만듦, n/2번 heapify()수행\n",
    "    for i <- ⌊n/2⌋ downto 1;     # 1 = 루트노드\n",
    "        heapify(A, i, n);       # 최악의 경우 트리의 높이인 log2^n-1까지 실행\n",
    "}"
   ]
  }
